    "clean-css": "4.2.1",
    "cross-env": "5.2.0",
    "directory-named-webpack-plugin": "4.0.0",
    "emittery": "0.4.1",
    "eslint": "5.9.0",
    "eslint-config-kentcdodds": "14.0.4",
