    "@babel/preset-env": "7.4.5",
    "@radum/eslint-config": "0.3.0",
    "@radum/stylelint-config": "0.3.0",
    "assets-webpack-plugin": "3.9.10",
    "ava": "2.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-console": "0.2.1",
    "browser-sync": "2.26.7",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chokidar": "3.0.1",
    "clean-css": "4.2.1",
    "core-js": "3.1.4",
    "cpy": "7.3.0",
    "cross-env": "5.2.0",
    "directory-named-webpack-plugin": "4.0.1",
    "emittery": "0.4.1",
    "eslint": "6.0.1",
