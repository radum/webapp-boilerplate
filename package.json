{
  "private": true,
  "name": "webapp-boilerplate-webpack",
  "version": "0.1.0",
  "description": "Scaffold out a front-end web app using Gulp & Webpack",
  "license": "MIT @ Radu Micu",
  "author": "Radu Micu",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "web",
    "app",
    "front-end",
    "h5bp",
    "modernizr",
    "jquery",
    "webpack",
    "gulp",
    "boilerplate",
    "starter",
    "web starter",
    "pwa",
    "progressive"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development node tools/start.js run dev",
    "dev:verbose": "npm run dev -- --verbose",
    "dev:analyze": "npm run dev -- --analyze",
    "build": "cross-env NODE_ENV=production node tools/start.js run build",
    "build:release": "npm run build -- --release",
    "build:analyze": "npm run build -- --analyze",
    "deploy:heroku": "git subtree push --prefix build heroku master"
  },
  "dependencies": {
