{
  "private": true,
  "name": "webapp-boilerplate-webpack",
  "version": "0.1.0",
  "description": "Scaffold out a front-end web app using Webpack, Express.js, and others",
  "license": "MIT @ Radu Micu",
  "author": "Radu Micu",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "web",
    "app",
    "front-end",
    "h5bp",
    "modernizr",
    "jquery",
    "webpack",
    "boilerplate",
    "starter",
    "web starter",
    "pwa",
    "progressive"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development node tools/run.js dev",
    "dev:inspect": "cross-env NODE_ENV=development node --inspect-brk tools/run.js dev",
    "dev:verbose": "npm run dev -- --verbose",
    "dev:analyze": "npm run dev -- --analyze",
    "build": "cross-env NODE_ENV=production node tools/run.js build",
    "build:release": "cross-env NODE_ENV=production node tools/run.js build --release",
    "build:analyze": "npm run build -- --analyze",
    "deploy:heroku": "git subtree push --prefix build heroku master"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "chalk": "~2.3.1",
    "compression": "~1.7.2",
    "cookie-parser": "~1.4.3",
    "dotenv": "~5.0.0",
    "errorhandler": "~1.5.0",
    "express": "~4.16.2",
    "express-minify-html": "~0.12.0",
    "express-session": "~1.15.6",
    "express-status-monitor": "~1.0.1",
    "lodash": "~4.17.5",
    "lusca": "~1.5.2",
    "marko": "~4.8.0",
    "morgan": "~1.9.0",
    "pretty-error": "~2.1.1",
    "spdy": "~3.4.7",
    "time-stamp": "~2.0.0"
  },
  "devDependencies": {
    "@babel/core": "~7.0.0-beta.40",
    "@babel/preset-env": "~7.0.0-beta.40",
    "assets-webpack-plugin": "~3.5.1",
    "babel-loader": "~8.0.0-beta.2",
    "babel-minify-webpack-plugin": "~0.3.0",
    "browser-sync": "~2.23.6",
    "case-sensitive-paths-webpack-plugin": "~2.1.1",
    "chokidar": "~2.0.2",
    "clean-css": "~4.1.9",
    "cross-env": "~5.1.3",
    "debug": "~3.1.0",
    "emittery": "~0.3.0",
    "eslint": "~4.18.1",
    "eslint-config-airbnb-base": "~12.1.0",
    "eslint-plugin-compat": "~2.2.0",
    "eslint-plugin-import": "~2.9.0",
    "execa": "~0.9.0",
    "favicons": "~4.8.6",
    "figures": "~2.0.0",
    "globby": "~8.0.1",
    "html-webpack-plugin": "~2.30.1",
    "imagemin": "~5.3.1",
    "imagemin-mozjpeg": "~7.0.0",
    "imagemin-optipng": "~5.2.1",
    "indent-string": "~3.2.0",
    "meow": "~4.0.0",
    "mkdirp": "~0.5.1",
    "ms": "~2.1.1",
    "node-sass": "~4.7.2",
    "ora": "~2.0.0",
    "os": "~0.1.1",
    "p-map": "~1.2.0",
    "postcss-custom-properties": "~7.0.0",
    "postcss-flexbugs-fixes": "~3.3.0",
    "postcss-reporter": "~5.0.0",
    "postcss-scss": "~1.0.4",
    "pretty-bytes": "~4.0.2",
    "randoma": "~1.1.0",
    "raw-loader": "~0.5.1",
    "require-dir": "~1.0.0",
    "rimraf": "~2.6.2",
    "sass-mq": "~4.0.2",
    "stylelint": "~9.1.1",
    "stylelint-no-unsupported-browser-features": "~2.0.0",
    "stylelint-scss": "~2.4.0",
    "supports-color": "~5.2.0",
    "webpack": "~4.0.1",
    "webpack-bundle-analyzer": "~2.11.0",
    "webpack-dev-middleware": "~2.0.6",
    "webpack-hot-middleware": "~2.21.0",
    "webpack-jarvis": "~0.3.0",
    "webpack-manifest-plugin": "~1.3.2",
    "webpack-monitor": "~1.0.14",
    "workbox-webpack-plugin": "~2.1.2",
    "write-file-webpack-plugin": "~4.2.0"
  }
}
