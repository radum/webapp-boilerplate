{
  "private": true,
  "name": "webapp-boilerplate",
  "version": "0.1.0",
  "description": "Scaffold out a front-end web app using Webpack, Express.js, and others",
  "license": "MIT @ Radu Micu",
  "author": "Radu Micu",
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "keywords": [
    "web",
    "app",
    "front-end",
    "h5bp",
    "modernizr",
    "jquery",
    "webpack",
    "boilerplate",
    "starter",
    "web starter",
    "pwa",
    "progressive"
  ],
  "scripts": {
    "taskrunner": "cross-env NODE_ENV=development node tools/run.js",
    "dev": "cross-env NODE_ENV=development node tools/run.js dev",
    "dev:inspect": "cross-env NODE_ENV=development node tools/run.js dev --inspect",
    "dev:verbose": "npm run dev -- --verbose",
    "dev:analyze": "npm run dev -- --analyze",
    "dev:lint": "npm run taskrunner -- lint",
    "build": "cross-env NODE_ENV=production node tools/run.js build",
    "build:release": "cross-env NODE_ENV=production node tools/run.js build --release",
    "build:analyze": "npm run build -- --analyze",
    "analyze:bundle-buddy": "npx bundle-buddy build/static/scripts/*.map",
    "analyze:slow-deps": "npx slow-deps",
    "deploy:heroku": "git subtree push --prefix build heroku master",
    "test": "ava --verbose"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.55",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-minify-html": "^0.12.0",
    "express-rate-limit": "^2.11.0",
    "express-session": "^1.15.6",
    "express-status-monitor": "^1.1.5",
    "helmet": "^3.13.0",
    "joi": "^13.5.2",
    "lit-html": "^0.10.2",
    "lodash": "^4.17.10",
    "lusca": "^1.6.1",
    "marko": "^4.12.4",
    "morgan": "^1.9.0",
    "pretty-error": "^2.1.1",
    "raven-js": "^3.26.4",
    "response-time": "^2.3.2",
    "server-timing": "^2.1.0",
    "spdy": "^3.4.7",
    "time-stamp": "^2.0.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.55",
