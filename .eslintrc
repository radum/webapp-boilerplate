{
  "extends": "airbnb-base",
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["compat"],
  "rules": {
	"compat/compat": "warn",
    "comma-dangle": "off",
    "prefer-template": "off",
	"no-tabs": "off",
    "indent": [2, "tab", { "SwitchCase": 1, "VariableDeclarator": 1 }],
    "arrow-body-style": [1, "as-needed"],
    "max-len": [2, 156, 2, {
      "ignoreUrls": true,
      "ignoreComments": true
    }]
  }
}
